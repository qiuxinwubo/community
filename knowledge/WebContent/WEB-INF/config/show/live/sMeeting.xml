<?xml version="1.0" encoding="UTF-8"?>
<webpage>
	<code>sMeeting</code>
	<url>/live/sMeeting</url>
	<datasets>
		<dataset>
			<name>ds1</name>
			<type>1</type>
			<scope>3</scope>
			<pageSize>10</pageSize>
			<pageNoArg>pageNo</pageNoArg>
			<sql>
			<![CDATA[
			       select DISTINCT(v.id) as idd,v.*,t.win_name,(select count(*) FROM VMS_MEETING_ATTENDEE s where s.video_id=v.id) as cou 
			       from VMS_LIVE_INFO v,VMS_MEETING_ATTENDEE t where v.START_TIME>sysdate and OPENDED='false' 
			       and v.id = t.video_id and (t.win_id='$user.orgId$' or '$user.belongorgType$' = 6) and  if_del='1' 
			       and IF_STATUS='2' ${1} ${2} ${3} order by CREATE_TIME
			]]>
			</sql>	
	   <sqlparts>
				<sqlpart>
					<part>${1}</part>
					<type>1</type>
					<arg>subject</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and v.subject like ? </value>
						</option>
					</options>
				</sqlpart>
				<sqlpart>
					<part>${2}</part>
					<type>1</type>
					<arg>startTimeStart</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and V.START_TIME &gt;=to_date(?,'YYYY-MM-DD hh24:mi:ss')</value>
						</option>
					</options>
				</sqlpart>
				<sqlpart>
					<part>${3}</part>
					<type>1</type>
					<arg>startTimeEnd</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and V.END_TIME &lt;=to_date(?,'YYYY-MM-DD hh24:mi:ss')</value>
						</option>
					</options>
				</sqlpart>
			</sqlparts>
			<vars>	
				<var>
					<argName>subject</argName>
					<type>1</type>
					<prefix>%</prefix>
					<suffix>%</suffix>
				</var>
				<var>
					<argName>startTimeStart</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix></suffix>
				</var>
					<var>
					<argName>startTimeEnd</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix></suffix>
				</var>
			</vars>
		</dataset>
	</datasets>
</webpage>