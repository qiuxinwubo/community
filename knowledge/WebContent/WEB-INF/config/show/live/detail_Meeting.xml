<?xml version="1.0" encoding="UTF-8"?>
<webpage>
	<code>detail_Meeting</code>
	<url>/live/detail_Meeting</url>
	<datasets>
		<dataset>
			<name>ds1</name>
			<type>1</type>
			<scope>2</scope>
			<sql>
			<![CDATA[
                    select * 
                    from VMS_LIVE_INFO V 
					where 1=1 ${1}
			]]>
			</sql>
			<sqlparts>
				<sqlpart>
					<part>${1}</part>
					<type>1</type>
					<arg>id</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and V.id = ? </value>
						</option>
					</options>
				</sqlpart>
			</sqlparts>
			<vars>	
				<var>
					<argName>id</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix></suffix>
				</var>
			</vars>
		</dataset>
		<dataset>
			<name>ds2</name>
			<type>1</type>
			<scope>4</scope>
			<pageSize>1</pageSize>
			<sql>
			<![CDATA[
					select w.win_name  from common_window w,VMS_MEETING_ATTENDEE s
					where 1=1 ${1} and w.id= s.win_id
			]]>
			</sql>
			<sqlparts>
				<sqlpart>
					<part>${1}</part>
					<type>1</type>
					<arg>id</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and s.video_id=? </value>
						</option>
					</options>
				</sqlpart>
			</sqlparts>
			<vars>	
				<var>
					<argName>id</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix></suffix>
				</var>
			</vars>
		</dataset>
	</datasets>
</webpage>