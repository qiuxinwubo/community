<?xml version="1.0" encoding="UTF-8"?>
<webpage>
	<code>cast_list</code>
	<url>/cast/cast_list</url>
	<datasets>
		<dataset>
			<name>ds1</name>
			<type>1</type>
			<scope>3</scope>
			<pageSize>10</pageSize>
			<pageNoArg>pageNo</pageNoArg>
			<sql>
			<![CDATA[
			select * from vms_cast_info t 
			where t.if_del='1' ${1}${2}${3}${4}${5}${6}
			order by t.if_shelves asc,t.serial_number asc, t.vid_createtime desc
			]]>
			</sql>
			<sqlparts>
				<sqlpart>
					<part>${1}</part>
					<type>1</type>
					<arg>headline</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and t.name like ? </value>
						</option>
					</options>
				</sqlpart>
				<sqlpart>
					<part>${2}</part>
					<type>1</type>
					<arg>uploadTime</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and t.vid_createtime &gt;  to_date(?,'yyyy-mm-dd')</value>
						</option>
					</options>
				</sqlpart>
				<sqlpart>
					<part>${3}</part>
					<type>1</type>
					<arg>uploadTime1</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and t.vid_createtime &lt;=  to_date(?,'yyyy-mm-dd hh24:mi:ss')</value>
						</option>
					</options>
				</sqlpart>
				<sqlpart>
					<part>${4}</part>
					<type>1</type>
					<arg>servicetypes</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and t.service_type = ?</value>
						</option>
					</options>
				</sqlpart>
				<sqlpart>
					<part>${5}</part>
					<type>1</type>
					<arg>speakerName</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and t.speaker like ?</value>
						</option>
					</options>
				</sqlpart>
				<sqlpart>
					<part>${6}</part>
					<type>1</type>
					<arg>shelves</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and t.IF_SHELVES = ?</value>
						</option>
					</options>
				</sqlpart>
			</sqlparts>
			<vars>	
				<var>
					<argName>headline</argName>
					<type>1</type>
					<prefix>%</prefix>
					<suffix>%</suffix>
				</var>
				<var>
					<argName>uploadTime</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix></suffix>
				</var>
				<var>
					<argName>uploadTime1</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix> 23:59:59</suffix>
				</var>
				<var>
					<argName>servicetypes</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix></suffix>
				</var>
				<var>
					<argName>speakerName</argName>
					<type>1</type>
					<prefix>%</prefix>
					<suffix>%</suffix>
				</var>
				<var>
					<argName>shelves</argName>
					<type>1</type>
					<prefix></prefix>
					<suffix></suffix>
				</var>
			</vars>
			<transcodes>
				<transcode>
					<code>service_type</code>
					<type>2</type>
					<transcode>select service_type_name from VMS_DIC_SERVICE_TYPE  where service_type_code='$service_type$'</transcode>
				</transcode>
			</transcodes>
		</dataset>
		
		<dataset>
			<name>ds2</name>
			<type>1</type>
			<scope>4</scope>
			<sql>
			<![CDATA[
			select * from vms_dic_service_type t where lev = '2' order by t.service_type_code
			]]>
			</sql>
		</dataset>
	</datasets>
</webpage>