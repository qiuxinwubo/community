<?xml version="1.0" encoding="UTF-8"?>
<webpage>
	<code>ba_pproduct_list</code>
	<url>/backstage/ba_pproduct_list</url>
	<datasets>
		<dataset>
			<name>ds1</name>
			<type>1</type>
			<scope>3</scope>
			<pageSize>10</pageSize>
			<pageNoArg>pageNo</pageNoArg>
			<sql>
			<![CDATA[
				select * 
				from IP_INTELLECTUAL_PROPERTY t 
				where if_del='0' and (state='2' or state='3') ${1} 
				order by state asc,create_time desc
			]]>
			</sql>
			<sqlparts>
				<sqlpart>
					<part>${1}</part>
					<type>1</type>
					<arg>name</arg>
					<defaultValue></defaultValue>
					<options>
						<option>
							<code>null</code>
							<value></value>
						</option>
						<option>
							<code>not null</code>
							<value> and t.name like ? </value>
						</option>
					</options>
				</sqlpart>
			</sqlparts>
			<vars>	
				<var>
					<argName>name</argName>
					<type>1</type>
					<prefix>%</prefix>
					<suffix>%</suffix>
				</var>
			</vars>
		</dataset>
	</datasets>
</webpage>